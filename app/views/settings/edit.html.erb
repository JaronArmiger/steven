<%= form_with(model: @setting, local: true) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
	<div class="user-color">
	  <%= f.label(:color) %>
	  <%= f.color_field :color %>
	  <div class="post-form-buttons">
		<%= f.submit "Save Changes", class: "color-button" %>
	  </div>
	</div>
	
<% end %>